@model IEnumerable<ApplicationUser>

<div class="list-group">
   @foreach (var follow in Model)
   {
       <a asp-controller="Account" asp-action="Profile" asp-route-id="@follow.Id" class="list-group-item">
           <div class="row">
               <div class="col-md-1">
                   <img src="~/ProfilePics/@follow.PhotoPath" class="img-responsive" style="max-height: 60px;">
               </div>
               <div class="col-md-11">
                    <h4>@follow.Firstname @follow.Lastname</h4>
               </div>   
           </div>
       </a>
   }    
</div>

